	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#6e7781"
			}
		},
		{
			"scope": [
				"constant.other.placeholder",
				"constant.character"
			],
			"settings": {
				"foreground": "#cf222e"
			}
		},
		{
			"scope": [
