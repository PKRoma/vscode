{
	"name": "mock-chat-provider",
	"displayName": "Mock Chat Provider",
	"description": "Provides mock auth and chat for agent sessions E2E testing",
	"version": "0.0.1",
	"publisher": "test",
	"engines": { "vscode": "^1.100.0" },
	"activationEvents": ["*"],
	"main": "./extension.js",
	"enabledApiProposals": ["defaultChatParticipant", "chatParticipantAdditions", "chatParticipantPrivate"],
	"contributes": {
		"authentication": [
			{
				"id": "github",
				"label": "GitHub (Mock)"
			}
		],
		"chatParticipants": [
			{
				"id": "mock-chat-provider.mock",
				"fullName": "Mock Agent",
				"name": "mock",
				"description": "Mock chat participant for testing",
				"isDefault": true
			}
		]
	}
}
